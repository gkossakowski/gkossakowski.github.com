    "autoprefixer": "10.4.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.4.5",
    "babel-preset-gatsby": "2.4.0",
    "browserslist": "4.19.1",
    "concurrently": "6.4.0",
    "eslint": "8.4.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.3.0",
